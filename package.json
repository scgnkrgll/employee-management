{
  "name": "@lit/lit-starter-js",
  "private": true,
  "version": "2.0.2",
  "description": "An employee directory project",
  "main": "my-element.js",
  "module": "my-element.js",
  "type": "module",
  "scripts": {
    "build": "npm run build:clean && npm run build:lit-localize && npm run build:bundle",
    "build:bundle": "rollup -c",
    "build:clean": "rimraf dist",
    "build:lit-localize": "lit-localize build",
    "lint": "npm run lint:lit-analyzer && npm run lint:eslint",
    "lint:eslint": "eslint '**/*.js'",
    "lint:lit-analyzer": "lit-analyzer src/",
    "format": "prettier \"**/*.{cjs,html,js,json,md,ts}\" --ignore-path ./.eslintignore --write",
    "serve": "wds --watch",
    "serve:prod": "MODE=prod npm run serve",
    "serve:dist": "wds --root-dir dist",
    "test": "npm run test:dev && npm run test:prod",
    "test:dev": "wtr",
    "test:watch": "wtr --watch",
    "test:prod": "MODE=prod wtr",
    "test:prod:watch": "MODE=prod wtr --watch",
    "extract": "lit-localize extract"
  },
  "keywords": [
    "web-components",
    "lit-element",
    "javascript",
    "lit"
  ],
  "author": "Google LLC",
  "license": "BSD-3-Clause",
  "dependencies": {
    "@lit/context": "^1.1.6",
    "@lit/localize": "^0.12.2",
    "@reduxjs/toolkit": "^2.8.2",
    "@vaadin/router": "^2.0.0",
    "fuse.js": "^7.1.0",
    "lit": "^3.2.0",
    "pwa-helpers": "^0.9.1",
    "zod": "^4.1.5"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.17.0",
    "@custom-elements-manifest/analyzer": "^0.6.3",
    "@lit/localize-tools": "^0.8.0",
    "@open-wc/testing": "^4.0.0",
    "@rollup-extras/plugin-copy": "^1.11.1",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-replace": "^5.0.7",
    "@rollup/plugin-terser": "^0.4.4",
    "@web/dev-server": "^0.1.31",
    "@web/dev-server-legacy": "^1.0.0",
    "@web/test-runner": "^0.15.0",
    "@web/test-runner-playwright": "^0.9.0",
    "@webcomponents/webcomponentsjs": "^2.8.0",
    "eslint": "^8.15.0",
    "lit-analyzer": "^1.2.1",
    "prettier": "^2.6.2",
    "rimraf": "^3.0.2",
    "rollup": "^4.50.0",
    "rollup-plugin-summary": "^2.0.1"
  },
  "customElements": "custom-elements.json"
}
